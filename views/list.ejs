
<%- include("header") -%>


    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>

    <div class="box">
      <%newlistItems.forEach((item)=>{ %>

      <div class="item">
        <input type="checkbox" />
        <p><%= item %></p>
      
      </div>
      <% }) %>

      <form class="item" method="post" action="">
        <input type="text" name="listItem" placeholder="New Item" autocomplete="off" />
        <button type="submit">+</button>
      </form>
    </div>

    <%- include("footer") -%>


